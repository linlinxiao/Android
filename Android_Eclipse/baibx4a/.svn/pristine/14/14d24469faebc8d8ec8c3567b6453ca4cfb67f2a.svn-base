<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="讲师级别编辑完成-6"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row6"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/mycard_back" >

        <TextView
            android:id="@+id/tv_p6_t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/iv_mycard_p6_v2"
            android:layout_alignRight="@+id/iv_mycard_p6_v2"
            android:layout_marginBottom="10dp"
            android:clickable="true"
            android:onClick="p6_t1"
            android:text="可编辑"
            android:textColor="#ff8d09"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_mycard_p6_v2"
            android:layout_width="160dp"
            android:layout_height="195dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/iv_mycard_p6_v1"
            android:layout_marginRight="20dp"
            android:background="@drawable/mycard_p6_v2"
            android:clickable="true"
            android:onClick="p6_v2" />

        <ImageView
            android:id="@+id/iv_mycard_p6_v1"
            android:layout_width="155dp"
            android:layout_height="115dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:background="@drawable/mycard_p6_v1"
            android:clickable="true"
            android:onClick="p6_v1" />

        <TextView
            android:id="@+id/tv_p6_t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/iv_mycard_p6_v1"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/iv_mycard_p6_v2"
            android:layout_marginBottom="15dp"
            android:clickable="true"
            android:onClick="p6_t2"
            android:text="可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑"
            android:textColor="#ffffff"
            android:textSize="14sp" />
    </RelativeLayout>
</LinearLayout>