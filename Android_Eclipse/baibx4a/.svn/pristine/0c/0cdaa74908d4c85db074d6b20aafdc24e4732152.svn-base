<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
        <RelativeLayout
	        android:id="@+id/title1"
	        android:layout_width="match_parent"
	        android:layout_height="50dp"
	        android:background="#2299EE"
	        android:orientation="horizontal" >
        <ImageView
			android:onClick="invitation_wine_back"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@drawable/jianjieback" />
        <TextView android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="邀请函编辑"
            android:textColor="#ffffff"
            android:textSize="20sp" />
        <TextView
            android:onClick="preView"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="预览"
            android:textColor="#ffffff"
            android:textSize="16sp"
            android:paddingRight="20dp"
            android:paddingLeft="10dp"
            />
    </RelativeLayout>
	<ScrollView android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:background="#efeff4">
		<LinearLayout
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="vertical">
			<View 
			    android:layout_marginTop="15dp"
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="45dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="主题"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_subject"
		            android:text="酒会邀请函"
		            android:paddingRight="15dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入主题"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
			<View 
			    android:layout_marginTop="20dp"
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="30dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="邀请函基本信息"
		            android:paddingLeft="15dp"
		            android:textSize="14sp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="主办人"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_sponsor"
		            android:onClick="sponsor"
		            android:clickable="true"
		            android:text="2016"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入姓名"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
					    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="酒会名称"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_winePartyName"
		            android:onClick="winePartyName"
		            android:clickable="true"
		            android:text="客户答谢会"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入姓名"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
					    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="被邀请人姓名"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_inviteeName"
		            android:onClick="inviteeName"
		            android:clickable="true"
		            android:text="张伟"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入姓名"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="姓别"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <RadioGroup 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"
		            android:orientation="horizontal"
		            android:layout_marginRight="15dp">
		            <RadioButton 
		                android:id="@+id/rb_male"
		                android:button="@null"
		                android:paddingLeft="30dp"
		                android:background="@drawable/mycard_rb_selector"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="20dp"
		                android:checked="true"
		                android:textColor="#2299ee"
		                android:textSize="13sp"
		                android:text="男"/>
		            <RadioButton 
		                android:id="@+id/rb_female"
		                android:button="@null"
		                android:paddingLeft="30dp"
		                android:background="@drawable/mycard_rb_selector"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:textColor="#888888"
		                android:textSize="14sp"
		                android:text="女"/>
		        </RadioGroup>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="参会时间"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_meetingTime"
		            android:onClick="meetingTime"
		            android:clickable="true"
		            android:text="2016年5月24号 16:30"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入参会时间"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="参会地址名称"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_meetingAddressName"
		            android:onClick="meetingAddressName"
		            android:clickable="true"
		            android:text="中国平安"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="wrap_content"
		            android:layout_height="match_parent"
		            android:hint="请输入参会地址名称"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="44dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="参会地址"
		            android:paddingLeft="15dp"
		            android:textSize="17sp"
		            android:textColor="#000000"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <TextView 
		            android:id="@+id/et_meetingAddress"
		            android:onClick="meetingAddress"
		            android:clickable="true"
		            android:text="北京市东花市北里20号楼6单元501室"
		            android:paddingRight="26dp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_alignParentRight="true"
		            android:gravity="right|center_vertical"
		            android:layout_width="175dp"
		            android:layout_height="match_parent"
		            android:hint="请输入参会地址"/>
		        <ImageView android:layout_width="8dp"
		            android:layout_height="16dp"
		            android:layout_alignParentRight="true"
		            android:layout_marginRight="12dp"
		            android:layout_centerVertical="true"
		            android:src="@drawable/arrow_right"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
		    <RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="30dp"
		        android:layout_marginTop="20dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="客户答谢酒会邀请函内容"
		            android:paddingLeft="15dp"
		            android:textSize="14sp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
	        <TextView 
	            android:id="@+id/et_content"
	            android:onClick="content"
	            android:clickable="true"
	            android:text="一路走来，您的殷切关怀是我努力进取的动力；您的大力支持是我取得成功的基石；您的倾心帮助是我敢于不断超越的源泉！我的成长离不开您的一切一切，为了感恩回馈您的支持帮助与理解，本人将举办一场客户答谢会，现诚邀您参加，真诚期待您的准时光临！"
	            android:paddingLeft="14dp"
	            android:textSize="15sp"
	            android:textColor="#888888"
	            android:layout_centerVertical="true"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:padding="10dp"
	            android:background="#ffffff"/>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"
			    android:layout_marginTop="20dp"/>
			<RelativeLayout 
		        android:layout_width="match_parent"
		        android:layout_height="30dp"
		        android:background="#ffffff">
		        <TextView 
		            android:text="会议流程"
		            android:paddingLeft="15dp"
		            android:textSize="14sp"
		            android:textColor="#888888"
		            android:layout_centerVertical="true"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"/>
		        <RadioGroup 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_alignParentRight="true"
		            android:layout_centerVertical="true"
		            android:orientation="horizontal"
		            android:layout_marginRight="15dp">
		            <RadioButton 
		                android:id="@+id/rb_appear"
		                android:button="@null"
		                android:paddingLeft="30dp"
		                android:background="@drawable/mycard_rb_selector"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_marginRight="20dp"
		                android:checked="true"
		                android:textColor="#2299ee"
		                android:textSize="13sp"
		                android:text="出现"/>
		            <RadioButton 
		                android:id="@+id/rb_no_appear"
		                android:button="@null"
		                android:paddingLeft="30dp"
		                android:background="@drawable/mycard_rb_selector"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:textColor="#888888"
		                android:textSize="14sp"
		                android:text="不出现"/>
		        </RadioGroup>
		    </RelativeLayout>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"/>
	        <TextView 
	            android:id="@+id/et_meetingProcess"
	            android:onClick="meetingProcess"
	            android:clickable="true"
	            android:text=""
	            android:paddingLeft="10dp"
	            android:paddingRight="10dp"
	            android:textSize="14sp"
	            android:textColor="#888888"
	            android:layout_centerVertical="true"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:background="#ffffff"/>
	        
	        <ImageView 
				android:onClick="iv_meetingProcess"
	            android:clickable="true"
	            android:layout_width="match_parent"
	            android:layout_height="wrap_content"
	            android:src="@drawable/invitation_editer_icon"
	            android:background="#ffffff"
	            android:paddingBottom="10dp"
	            android:layout_gravity="center"/>
			<View 
			    android:layout_width="match_parent" 
			    android:layout_height="1px"
			    android:background="#d9d9d9"
			    android:layout_marginBottom="20dp"/>
			
		</LinearLayout>
	</ScrollView>
</LinearLayout>