<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="钻石会员编辑完成-5"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row5"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/mycard_back" >

        <ImageView
            android:id="@+id/iv_mycard_p5_v1"
            android:layout_width="162dp"
            android:layout_height="115dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="110dp"
            android:background="@drawable/mycard_p5_v1"
            android:clickable="true"
            android:onClick="p5_v1" />

        <ImageView
            android:id="@+id/iv_mycard_p5_v2"
            android:layout_width="150dp"
            android:layout_height="115dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/iv_mycard_p5_v1"
            android:layout_marginRight="20dp"
            android:background="@drawable/mycard_p5_v2"
            android:clickable="true"
            android:onClick="p5_v2" />

        <ImageView
            android:id="@+id/iv_mycard_p5_v3"
            android:layout_width="150dp"
            android:layout_height="110dp"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/iv_mycard_p5_v2"
            android:layout_marginRight="20dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/mycard_p5_v3"
            android:clickable="true"
            android:onClick="p5_v3" />

        <TextView
            android:id="@+id/tv_p5_t1"
            android:layout_width="155dp"
            android:layout_height="100dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="140dp"
            android:layout_marginLeft="25dp"
            android:clickable="true"
            android:onClick="p5_t1"
            android:text="可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑"
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_p5_t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="90dp"
            android:layout_marginLeft="62dp"
            android:clickable="true"
            android:onClick="p5_t2"
            android:text="可编辑"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </RelativeLayout>
</LinearLayout>