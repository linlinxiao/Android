                 android:layout_height="fill_parent"
                 android:textSize="10sp"
            android:text="Bx(C+D+E)-e"
            android:textColor="#104E8B"
            android:textStyle="bold"
            />
            
        </LinearLayout>
        </LinearLayout>
    </LinearLayout>

