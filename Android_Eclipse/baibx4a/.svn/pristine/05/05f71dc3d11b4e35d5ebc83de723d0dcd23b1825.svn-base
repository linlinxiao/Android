<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/baiwanrwx_bg"
    android:orientation="horizontal"
    android:paddingRight="5dp" >

    <LinearLayout
        android:id="@+id/left"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="5dp"
        android:layout_weight="2"
        android:gravity="center_vertical" >

        <LinearLayout
            android:id="@+id/ll_circle"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:background="@drawable/hushenfu_circle"
            android:layout_marginLeft="55dp"
            android:layout_marginTop="15dp"
            android:gravity="center"
            android:orientation="vertical" >

            <!-- 1 -->

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/floatText1"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="B"
                    android:textColor="#E10C1A"
                    android:textSize="13sp"
                    android:textStyle="bold" />
            </LinearLayout>
            <!-- 2 -->

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/floatText2"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="C+D+E"
                    android:textColor="#F87D20"
                    android:textSize="12sp" />
            </LinearLayout>
            <!-- 3 -->

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/floatText3"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="Bx(C+D+E)-e"
                    android:textColor="#E10C1A"
                    android:textSize="13sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:onClick="right"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="4"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:orientation="vertical"
        >
        <LinearLayout
        android:id="@+id/leftLinear"
        android:layout_width="390dp"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:background="@drawable/baiwanrwx_top"
        android:layout_marginLeft="30dp" >
            
        	<LinearLayout 
        	   android:id="@+id/rl_top1"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.8"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:id="@+id/tv_common"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="普通意外"
        	        android:textColor="#000000"
        	        android:layout_marginTop="98dp"
        	        android:layout_marginLeft="33dp"
        	        android:textSize="11sp"
        	        
        	        />
        	    <TextView 
        	        android:id="@+id/tv_shengGu"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="身故/伤残"
        	        android:layout_marginLeft="30dp"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="保障金"
        	        android:textColor="#000000"
        	        android:layout_marginLeft="36dp"
        	        android:textSize="11sp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="10万"
        	        android:textColor="#000000"
        	        android:layout_marginBottom="5dp"
        	        android:layout_marginLeft="40dp"
        	        android:textSize="11sp"
        	        />
        	</LinearLayout>
        	<LinearLayout 
        	    android:id="@+id/rl_top2"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.5"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="满期生存金"
        	        android:textColor="#000000"
        	        android:layout_gravity="center"
        	        android:layout_marginTop="16dp"
        	        android:textSize="11sp"
        	        android:layout_marginLeft="10dp"
        	        />
        	    <TextView 
        	        android:id="@+id/tv_shengCunJin"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="10w*1.2"
        	        android:layout_gravity="center"
        	        android:textColor="#000000"
        	        android:layout_marginBottom="50dp"
        	        android:textSize="11sp"
        	        android:layout_marginLeft="10dp"
        	        />
        	</LinearLayout>
        	<LinearLayout 
        	    android:id="@+id/rl_top3"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.8"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="疾病"
        	        android:layout_gravity="center"
        	        android:textColor="#000000"
        	        android:layout_above="@+id/tv_shenGuBaoZhangJin"
        	        android:layout_centerHorizontal="true"
        	        android:layout_marginTop="101dp"
        	        android:layout_marginLeft="14dp"
        	        android:textSize="11sp"
        	        />
        	    <TextView 
        	        android:id="@+id/tv_shenGuBaoZhangJin"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="身故保障金"
        	        android:textColor="#000000"
        	        android:layout_centerInParent="true"
        	        android:layout_gravity="center"
        	        android:layout_marginLeft="18dp"
        	        android:textSize="11sp"
        	        />
        	    <TextView 
        	        android:id="@+id/tv_jiBingSCJ"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="所交保费的1.2倍"
        	        android:textColor="#000000"
        	        android:layout_alignParentBottom="true"
        	        android:layout_centerHorizontal="true"
        	        android:layout_marginBottom="5dp"
        	        android:layout_gravity="center"
        	        android:layout_marginLeft="25dp"
        	        android:textSize="11sp"
        	        
        	        />
        	</LinearLayout>
        	<LinearLayout 
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.1"
        	    ></LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/RightLinear"
        android:layout_width="390dp"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:orientation="horizontal"
        android:background="@drawable/baiwanrwx_bottom"
        android:layout_marginLeft="30dp" 
        >
        	<LinearLayout 
        	    android:id="@+id/rl_bottom1"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="1"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:id="@+id/tv_hangKongYiWai"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="航空意外"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginTop="55dp"
        	        android:layout_marginLeft="14dp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="身故/伤残保障金"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginLeft="14dp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="100万"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginLeft="14dp"
        	        />
        	</LinearLayout>
        	<LinearLayout 
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.1"
        	    ></LinearLayout>
        	<LinearLayout 
        	    android:id="@+id/rl_bottom2"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="0.9"
        	    android:layout_marginTop="10dp"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="100万"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_marginTop="95dp"
        	        android:layout_gravity="center"
        	        />
        	    <TextView 
        	        android:id="@+id/tv_ziJia"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="自驾车意外"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        />
        	    <TextView 
        	        android:id="@+id/tv_baoZhangJin"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="身故/全残保障金"
        	        android:textColor="#000000"
        	        android:layout_gravity="center"
        	        android:textSize="11sp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="(包含乘坐私家车的意外)"
        	        android:textColor="#000000"
        	        android:textSize="9sp"
        	        android:layout_gravity="center"
        	        />
        	</LinearLayout>
        	<LinearLayout 
        	    android:id="@+id/rl_bottom3"
        	    android:layout_width="0dp"
        	    android:layout_height="match_parent"
        	    android:layout_weight="1"
        	    android:orientation="vertical"
        	    >
        	    <TextView 
        	        android:id="@+id/tv_gongGongJiaoTong"
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="公共交通意外"
        	        android:textColor="#000000"
        	        android:layout_marginTop="53dp"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginRight="12dp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="身故/伤残保障金"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginRight="12dp"
        	        />
        	    <TextView 
        	        android:layout_width="wrap_content"
        	        android:layout_height="wrap_content"
        	        android:text="100万"
        	        android:textColor="#000000"
        	        android:textSize="11sp"
        	        android:layout_gravity="center"
        	        android:layout_marginRight="12dp"
        	        />
        	</LinearLayout>
    </LinearLayout>
    </LinearLayout>
    

</LinearLayout>