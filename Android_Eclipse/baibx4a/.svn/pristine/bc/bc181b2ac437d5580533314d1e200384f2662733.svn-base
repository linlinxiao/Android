<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="首页编辑完成-1"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000"
            android:visibility="gone" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/mycard_back_p1" >

        <TextView
            android:id="@+id/tv_mycard_p1_t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/tv_mycard_p1_t1"
            android:layout_alignTop="@+id/tv_mycard_p1_t1"
            android:layout_marginTop="30dp"
            android:clickable="true"
            android:onClick="p1_t2"
            android:text="可编辑"
            android:translationX="60dp"
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <com.baoxiao.view.RoundImageView
            android:id="@+id/iv_mycard_p1_v1"
            android:layout_width="190dp"
            android:layout_height="190dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="@drawable/mycard_head"
            android:translationY="-60dp"
            android:clickable="true"
            android:onClick="p1_v1" />

        <TextView
            android:id="@+id/tv_mycard_p1_t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_mycard_p1_v1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp"
            android:clickable="true"
            android:onClick="p1_t1"
            android:text="可编辑"
            android:translationX="60dp"
            android:textColor="#ffffff"
            android:textSize="24sp" />

    </RelativeLayout>
</LinearLayout>