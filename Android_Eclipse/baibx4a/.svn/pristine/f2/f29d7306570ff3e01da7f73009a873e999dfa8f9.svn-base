<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#F0EFF5"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/title1"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="#2299EE"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/shouHuXingBack"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:src="@drawable/shouhuxing_back" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="7"
            android:gravity="center"
            android:text="@string/TopTextView"
            android:textColor="#ffffff"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/shouHuXingMenu"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:src="@drawable/shouhuxing_menu" />
    </LinearLayout>
    
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scrollbars="none" 
        >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#F0EFF5"
        android:orientation="vertical"
        android:padding="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:paddingBottom="5dp"
            android:text="@string/JiBenQingKuang"
            android:textColor="#e94820" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="135dp"
            android:background="#ffffff"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:text="姓名"
                    style="@style/greyTV"
                    />
                <EditText 
                    android:id="@+id/name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="8"
                    android:background="@null"
                    />
                </LinearLayout>
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:text="性别"
                    style="@style/greyTV"
                     />

                <RadioGroup
                    android:id="@+id/sex"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="8"
                    android:orientation="horizontal" >

                    <RadioButton
                        android:id="@+id/maleRadioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="男" />

                    <RadioButton
                        android:id="@+id/femaleRadioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="女" />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:text="年龄"
                    style="@style/greyTV"
                    />

                <Spinner
                    android:id="@+id/age"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="8"
                    android:prompt="@string/age"
                    android:spinnerMode="dialog" 
                    />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="@string/ZhongShenShouXian"
            android:textColor="#e94820" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="90dp"
            android:background="#ffffff"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="4"
                    android:text="缴费年期"
                    android:textColor="#8E8E8E"
                    android:textSize="18sp" />

                <Spinner
                    android:id="@+id/period"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="6"
                    android:spinnerMode="dialog" 
                    android:prompt="@string/jiaoFeiNianQi"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:text="保额"
                    style="@style/greyTV"
                    />

                <EditText android:maxLength="6"
                    android:id="@+id/baoE"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="8"
                    android:background="@null"
                    android:inputType="number"  />
            <TextView
                android:text="万元"
                style="@style/greyTV"
                />                
                
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="@string/ZhongDaJiBingXian"
            android:textColor="#e94820" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:background="#FFFFFF"
            android:orientation="horizontal"
            >

            <TextView
                android:text="保额"
                style="@style/greyTV"
                />

            <EditText android:maxLength="6"
                android:id="@+id/jiBingBaoE"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="8"
                android:layout_gravity="center"
                android:background="@null"
                android:inputType="number"                
                />
            <TextView
                android:text="万元"
                style="@style/greyTV"
                />             
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:text="@string/FenHongShuiPing"
            android:textColor="#e94820" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="30dp"
            android:background="#DDDDDD"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/fenHongShuiPingLow"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/FenHongShuiPing_low"
                style="@style/blackTV"
                />

            <TextView
                android:id="@+id/fenHongShuiPingMiddle"
                android:layout_weight="1"
                android:background="#F3C095"
                android:gravity="center"
                android:text="@string/FenHongShuiPing_middle"
                style="@style/blackTV"
                />

            <TextView
                android:id="@+id/fenHongShuiPingHigh"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/FenHongShuiPing_high"
                style="@style/blackTV"
                />
        </LinearLayout>
    
    
     <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingTop="5dp" >

                    <TextView
                        android:id="@+id/ageShow"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:text="0岁"
                        android:textColor="#e94820" />

                    <TextView
                        android:layout_width="2dp"
                        android:layout_height="fill_parent" />

                    <TextView
                        android:id="@+id/sexShow"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:text="男性"
                        android:textColor="#e94820" />

                    <TextView
                        android:layout_width="2dp"
                        android:layout_height="fill_parent" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:text="计划书预览"
                        android:textColor="#e94820" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal" >

                        <ImageView
                            android:src="@drawable/shx_zhuxian"
                            style="@style/showImage"
                             />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="6"
                            android:orientation="vertical" >

                            <TextView
                                android:id="@+id/baoXianProductTitle"
                                android:layout_width="fill_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1.5"
                                android:gravity="center_vertical"
                                android:text="守护星少儿两全保险"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保额："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zhuXianBaoEShow"
                                    android:text="1000元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="交费年期："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zxPeriodShow"
                                    android:text="10年"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保费："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zxBaoFeiShow"
                                    android:text="243.80元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        style="@style/lineImage"
                         >
                    </ImageView>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal" >
                        

                        <ImageView
                            android:src="@drawable/shx_zhongji" 
                            style="@style/showImage"
                            />
                        

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="6"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1.5"
                                android:gravity="center_vertical"
                                android:text="附加少儿重大疾病保险（2014）"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保额："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zhongJiBaoEShow"
                                    android:text="1000元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="交费年期："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zjPeriodShow"
                                    android:text="10年"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保费："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/zjBaoFeiShow"
                                    android:text="243.80元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        style="@style/lineImage"
                      >
                    </ImageView>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="120dp"
                        android:layout_marginBottom="5dp"
                        android:layout_marginTop="5dp"
                        android:orientation="horizontal" >

                        <ImageView
                            android:src="@drawable/shx_huomian" 
                            style="@style/showImage"
                            />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="5dp"
                            android:layout_weight="6"
                            android:orientation="vertical" >

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1.5"
                                android:text="豁免保险（附加少儿豁免保险费重大疾病保险）"
                                android:textSize="16sp"
                                android:textStyle="bold" />

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保额："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/huoMianBaoEShow"
                                    android:text="1000元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="交费年期："
                                    style="@style/blackTV"
                                     />
                                <TextView
                                    android:id="@+id/hmPerioidShow"
                                    android:text="10年"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:orientation="horizontal" >

                                <TextView
                                    android:text="保费："
                                    style="@style/blackTV"
                                    />

                                <TextView
                                    android:id="@+id/hmBaoFeiShow"
                                    android:text="243.80元"
                                    style="@style/blackTV"
                                    />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <include layout="@layout/ps_plan_account"/>
                </LinearLayout>
            </LinearLayout>
    	
     	<include layout="@layout/ps_plan_confirm"/>
    </LinearLayout>
    </ScrollView>

</LinearLayout>