<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="十大展业能手编辑完成-7"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row7"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/mycard_back" >

        <TextView
            android:id="@+id/tv_p7_t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/iv_mycard_p7_v2"
            android:layout_alignRight="@+id/iv_mycard_p7_v2"
            android:layout_marginBottom="35dp"
            android:clickable="true"
            android:onClick="p7_t1"
            android:text="可编辑"
            android:textColor="#ff8d09"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_mycard_p7_v1"
            android:layout_width="150dp"
            android:layout_height="211dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="162dp"
            android:background="@drawable/mycard_editer1"
            android:clickable="true"
            android:onClick="p7_v1" />

        <ImageView
            android:id="@+id/iv_mycard_p7_v2"
            android:layout_width="156dp"
            android:layout_height="82dp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/iv_mycard_p7_v1"
            android:layout_marginRight="20dp"
            android:background="@drawable/mycard_p7_v2"
            android:clickable="true"
            android:onClick="p7_v2" />

        <ImageView
            android:id="@+id/iv_mycard_p7_v3"
            android:layout_width="156dp"
            android:layout_height="120dp"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/iv_mycard_p7_v2"
            android:layout_marginRight="20dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/mycard_p7_v3"
            android:clickable="true"
            android:onClick="p7_v3" />

        <TextView
            android:id="@+id/tv_p7_t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/iv_mycard_p7_v1"
            android:layout_alignRight="@+id/iv_mycard_p7_v3"
            android:layout_below="@+id/iv_mycard_p7_v3"
            android:layout_marginTop="26dp"
            android:clickable="true"
            android:onClick="p7_t2"
            android:text="可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑"
            android:textColor="#ffffff"
            android:textSize="14sp" />
    </RelativeLayout>
</LinearLayout>