		        android:layout_centerVertical="true"
		        android:layout_marginLeft="50dp"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textColor="#1f1f1f"
		        android:text="少儿平安福终身寿险"/>
		    
		</RelativeLayout>
