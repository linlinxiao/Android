			        style="@style/greyTV3"
			        android:text="保额"/>
			    <EditText
			        android:maxLength="6"
			        android:id="@+id/et_enjoy_health"
			        android:layout_width="0dp"
			        android:layout_height="match_parent"
			        android:layout_gravity="center_vertical"
			        android:paddingLeft="10dp"
			        android:layout_weight="2"
			        android:background="@null"
			        android:inputType="numberDecimal" 
			        android:hint="请输入保额"
			        android:textSize="13sp"
			        />
			    <TextView
		        	android:layout_weight="1"
			        style="@style/greyTV2"
			        android:text="份"/>
			    
			    <LinearLayout 
			        android:layout_height="wrap_content"
					android:layout_width="wrap_content"
			        android:orientation="vertical">
					<CheckBox
				        android:id="@+id/cb_enjoy_health_add"
				        android:button="@null"
				        android:textSize="13sp"
				        android:background="@drawable/peace_cb_selector3"
				        android:paddingLeft="35dp"
				        android:text="可选部分"
				        android:textColor="#1f1f1f"
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:checked="true"
				        android:layout_marginRight="10dp"/>
					<CheckBox
				        android:id="@+id/cb_enjoy_health_social"
				        android:button="@null"
				        android:textSize="13sp"
				        android:background="@drawable/peace_cb_selector3"
				        android:paddingLeft="35dp"
				        android:text="有社保"
				        android:textColor="#1f1f1f"
				        android:layout_width="wrap_content"
				        android:layout_height="match_parent"
				        android:checked="true"
				        android:layout_marginRight="10dp"/>
					</LinearLayout>
				</LinearLayout>			
			</LinearLayout>
			<Button 
			    android:onClick="add_other"
			    android:layout_width="match_parent"
			    android:layout_height="40dp"
			    android:text="添加其他附加险+"
			    android:background="#ffffff"
			    android:textSize="13sp"
			    android:textColor="#2299ee"/>
			<LinearLayout 
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:orientation="vertical"
			    android:background="#ffffff">
				<View
				    android:layout_width="match_parent" 
				    android:layout_height="1px"
				    android:background="#c8c8c8"/>
				
				<TextView
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="20dp"
			        android:layout_marginTop="10dp"
			        android:layout_marginBottom="10dp"
			        android:textColor="#1f1f1f"
			        android:text="分红水平"/>
				<View
				    android:layout_width="match_parent" 
				    android:layout_height="1px"
				    android:background="#c8c8c8"/>
				<RadioGroup 
				    android:id="@+id/rg_grade"
				    android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:orientation="horizontal"
				    android:layout_above="@+id/peace_submit"
				    android:layout_marginLeft="15dp"
				    android:layout_marginRight="15dp">
				    <RadioButton 
				    	android:id="@+id/rb_grade1"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_margin="10dp"
				        android:gravity="center"
				        android:layout_weight="1"
				        android:background="@drawable/rb_center_selector2"
				        android:button="@null"
				        android:paddingTop="5dp"
				        android:paddingBottom="5dp"
				        android:textColor="#0299EE"
				        android:textSize="13sp"
				        android:text="低"/>
				    <RadioButton 
				    	android:id="@+id/rb_grade2"
				        android:checked="true"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_margin="10dp"
				        android:gravity="center"
				        android:layout_weight="1"
				        android:background="@drawable/rb_center_selector2"
				        android:button="@null"
				        android:paddingTop="5dp"
				        android:paddingBottom="5dp"
				        android:textColor="#ffffff"
				        android:textSize="13sp"
				        android:text="中"/>
				    <RadioButton 
				    	android:id="@+id/rb_grade3"
				        android:layout_width="wrap_content"
				        android:layout_height="wrap_content"
				        android:layout_margin="10dp"
				        android:gravity="center"
				        android:layout_weight="1"
				        android:background="@drawable/rb_center_selector2"
				        android:button="@null"
				        android:paddingTop="5dp"
				        android:paddingBottom="5dp"
				        android:textColor="#0299EE"
				        android:textSize="13sp"
				        android:text="高"/>
				</RadioGroup>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
	
    <Button
        android:id="@+id/sjtsh_submit"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="40dip"
        android:layout_margin="10dip"
        android:onClick="sjtsh_submit"
        android:text="确			定"
        android:background="@drawable/btn_selecter"/>
</RelativeLayout>