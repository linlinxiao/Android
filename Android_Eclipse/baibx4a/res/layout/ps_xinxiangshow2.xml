<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/xinxiang_bg"
    android:orientation="horizontal"
    android:paddingRight="5dp" >

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_weight="1" 
        android:layout_marginBottom="30dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        >
        <LinearLayout 
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/xinxiang_circle"
            android:orientation="vertical"
            android:gravity="center"
            >
            <!-- 1 -->
        <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="fill_parent"
                 android:textSize="10sp"
                 android:textColor="#8B4C39"
                 android:textStyle="bold"
            android:id="@+id/floatText1"
            android:text="B"
            />
        </LinearLayout>
        <!-- 2 -->
         <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
          
           <TextView
               android:layout_width="wrap_content"
                 android:layout_height="fill_parent"
                 android:textSize="10sp"
            android:id="@+id/floatText2"
            android:text="C+D+E"
            android:textColor="#8B4C39"
            />
        </LinearLayout>
        <!-- 3 -->
            <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
        
           <TextView
            android:id="@+id/floatText3"
            android:layout_width="wrap_content"
                 android:layout_height="fill_parent"
                 android:textSize="10sp"
            android:text="Bx(C+D+E)-e"
            android:textColor="#8B4C39"
            />
            
        </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/leftLinear"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="10dp"
        android:layout_weight="2"
        android:orientation="vertical"
        android:paddingRight="2dp" >

        <LinearLayout
            android:id="@+id/view1"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@drawable/xinxiang_lefttop"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <LinearLayout style="@style/w_one" 
                >

                <TextView
                    android:id="@+id/huShenFuJianKangTitle"
                    style="@style/huShenFu_RedBigTitle"
                    android:text="满期生存金"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="60dp" />
            </LinearLayout>

            <LinearLayout style="@style/w_four" >

                <TextView
                    android:id="@+id/huShenFuJianKangText1"
                    style="@style/huShenFu_RedSmallTitle"
                    android:text="老有所依，让自己安享晚年"
                    android:layout_marginLeft="50dp"
                    android:textColor="#ffffff" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:paddingBottom="5dp"
                    android:paddingRight="50dp"
                    android:paddingTop="5dp" 
                    >
<!--                     android:gravity="center_horizontal" -->

                    <LinearLayout
                        android:id="@+id/yangLaoLinear"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/xinxiang_yanglaojin1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical"
                        android:layout_marginRight="10dp" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="养老金"
                            android:textColor="#104E8B"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/yangLaoJin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2B万元"
                            android:textColor="#FF0000"
                            android:textSize="12sp" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="保险满期，全部领取"
                                android:textSize="9sp"
                                android:textColor="#000000" />
                        </LinearLayout>
                    </LinearLayout>


                    
                </LinearLayout>
            </LinearLayout>

            <LinearLayout style="@style/w_one" >

                <TextView
                    android:id="@+id/huShenFuJianKangText2"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:text="可做未来养老金，也可以做应急金"
                    android:textSize="9sp"
                    android:layout_marginLeft="12dp"
                    android:textColor="#ffffff" />

                <TextView
                    android:layout_width="40dp"
                    android:layout_height="fill_parent"
                    android:background="@drawable/xinxiang_center1"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="养老" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_marginTop="4dp"
            android:layout_weight="1"
            android:background="@drawable/xinxiang_leftbuttom"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <LinearLayout style="@style/w_one" 
                >

                <TextView
                    android:id="@+id/huShenFuBaoZhangTitle"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:text="健康关爱金"
                    android:textColor="#ffffff"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:layout_marginLeft="60dp" />

                <TextView
                    android:layout_width="40dp"
                    android:layout_height="fill_parent"
                    android:background="@drawable/xinxiang_center3"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="健康" />
            </LinearLayout>

            <LinearLayout style="@style/w_four" >

                <TextView
                    android:id="@+id/huShenFuBaoZhangText1"
                    style="@style/huShenFu_RedSmallTitle"
                    android:text="为您一生的健康保驾护航"
                    android:textColor="#000000"
                    android:layout_marginLeft="55dp" />
<LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:paddingBottom="5dp"
                    android:paddingRight="10dp"
                    android:paddingTop="5dp" 
                    android:gravity="center_horizontal"
                    >

                    <LinearLayout
                        android:id="@+id/jianKangLinear"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/xinxiang_zhongji2"
                        android:gravity="center_horizontal"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="重疾金"
                            android:textColor="#104E8B"
                            android:textSize="12sp"
                            android:textStyle="bold"  />

                        <TextView
                            android:id="@+id/jKZhongJiJin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="3C万元"
                            android:textColor="#FF0000"
                            android:textSize="12sp" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="bottom|center_horizontal"
                            android:paddingBottom="5dp" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="(保险期间)"
                                android:textSize="9sp"
                                android:textColor="#000000" />
                        </LinearLayout>
                    </LinearLayout>


                    
                </LinearLayout>
            </LinearLayout>

            <LinearLayout style="@style/r_one" >

                <TextView
                    android:id="@+id/huShenFuBaoZhangText2"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:gravity="center_horizontal"
                    android:text="社保加重疾保障，让健康没有后顾之忧"
                    android:textSize="9sp"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/RightLinear"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="10dp"
        android:layout_weight="2"
        android:orientation="vertical"
        android:paddingLeft="2dp" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="2dp"
            android:layout_weight="1"
            android:background="@drawable/xinxiang_righttop"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/fengHongLinear"
                style="@style/r_one"
                android:gravity="center_vertical|right"
                >

                <TextView
                    android:id="@+id/huShenFuFenHongTitle"
                    style="@style/huShenFu_RedBigTitle"
                    android:text="保单累积交清增额账户"
                    android:textColor="#ffffff"
                    android:layout_marginRight="20dp" />

            </LinearLayout>

            <LinearLayout
                style="@style/r_four"
                android:id="@+id/huShenFuFenHongLinear1"
                >

                <TextView
                    android:id="@+id/huShenFuFenHongText1"
                    style="@style/huShenFu_RedSmallTitle"
                    android:layout_gravity="right"
                    android:text="让保障逐年递增"
                    android:textColor="#000000"
                    android:layout_marginRight="80dp"  />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:paddingLeft="50dp"
                    android:paddingTop="5dp" 
                    android:gravity="center_horizontal"
                    >

                    <LinearLayout
                    android:id="@+id/zhongShenFenHongLinear"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="2"
                    android:layout_marginRight="5dp"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/xinxiang_zenge1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" >

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:gravity="center_vertical"
                        android:text="累积红利交清增额保额"
                        android:textStyle="bold" 
                        android:textColor="#104E8B"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/zengEBaoE"
                        android:layout_width="fill_parent"
                        android:layout_height="0dp"
                        android:layout_weight="2"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="5dp"
                        android:gravity="right|center_vertical"
                        android:text="H万元"
                        android:textColor="#FF0000"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="bottom"
                        android:textColor="#008000"
                        android:textSize="7sp" />
                </LinearLayout>

                    <LinearLayout
                        android:id="@+id/zengELevel"
                        android:layout_width="30dp"
                        android:layout_height="fill_parent"
                        android:background="@drawable/xinxiang_levelbg1"
                        android:orientation="vertical"
                        >
                    	<TextView 
                    	    android:id="@+id/zengEHighLevel"
                    	    android:layout_width="fill_parent"
                    	    android:layout_height="0dp"
                    	    android:layout_weight="1"
                    	    android:text="高"
                    	    android:textColor="#ffffff"
                    	    android:gravity="center"
                    	    />
                    	<TextView 
                    	    android:id="@+id/zengEMiddleLevel"
                    	    android:layout_width="fill_parent"
                    	    android:layout_height="0dp"
                    	    android:layout_weight="1"
                    	    android:text="中"
                    	    android:textColor="#ffffff"
                    	    android:gravity="center"
                    	    />
                    	<TextView 
                    	    android:id="@+id/zengELowLevel"
                    	    android:layout_width="fill_parent"
                    	    android:layout_height="0dp"
                    	    android:layout_weight="1"
                    	    android:text="低"
                    	    android:textColor="#ffffff"
                    	    android:gravity="center"
                    	    />
                    </LinearLayout>

                    
                </LinearLayout>
            </LinearLayout>

            <LinearLayout style="@style/r_one" >

                <TextView
                    android:layout_width="40dp"
                    android:layout_height="fill_parent"
                    android:background="@drawable/xinxiang_center2"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="分红" />

                <TextView
                    android:id="@+id/huShenFuFenHongText2"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:text="保险会长大"
                    android:layout_marginRight="8dp"
                    android:textSize="9sp"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_marginTop="2dp"
            android:background="@drawable/xinxiang_rightbuttom"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <LinearLayout style="@style/r_one" 
                >

                <TextView
                    android:layout_width="40dp"
                    android:layout_height="fill_parent"
                    android:background="@drawable/xinxiang_center4"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="意外" />

                <TextView
                    android:id="@+id/huShenFuYiWaiTitle"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical|right"
                    android:text="生命价值保障金"
                    android:textColor="#ffffff"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    android:layout_marginRight="40dp"  />
            </LinearLayout>

            <LinearLayout
                style="@style/r_four"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/huShenFuYiWaiText1"
                    style="@style/huShenFu_RedSmallTitle"
                    android:layout_gravity="right"
                    android:layout_marginRight="40dp" 
                    android:text="人生无常，给家人一份长久的保障"
                    android:textColor="#ffffff" />

                    <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp" 
                    android:gravity="center_horizontal"
                    >

                    <LinearLayout
                        android:id="@+id/yiWaiLinear"
                        android:layout_width="140dp"
                        android:layout_height="90dp"
                        android:background="@drawable/xinxiang_baozhang1"
                        android:gravity="center_horizontal"
                        android:orientation="vertical" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="1.5dp"
                            android:text="风险保障金"
                            android:textColor="#104E8B"
                            android:textSize="12sp"
                            android:textStyle="bold"  />

                        <TextView
                            android:id="@+id/fengXianBaoZhangJin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="3B万元"
                            android:textColor="#FF0000"
                            android:textSize="12sp" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="bottom|center_horizontal"
                             >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="（18岁后，按保额三倍提供保障）"
                                android:textSize="9sp"
                                android:textColor="#000000"
                                android:layout_marginBottom="2dp" />
                        </LinearLayout>
                </LinearLayout>
                    
                </LinearLayout>
            </LinearLayout>

            <LinearLayout style="@style/r_one" >

                <TextView
                    android:id="@+id/huShenFuYiWaiText2"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:gravity="center_horizontal"
                    android:text="关爱自己 保障家人"
                    android:textSize="9sp"
                    android:textColor="#ffffff" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>