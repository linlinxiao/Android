<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="MDRT荣誉编辑完成-10"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row10"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/mycard_back" >

        <ImageView
            android:id="@+id/iv_mycard_p10_v1"
            android:layout_width="258dp"
            android:layout_height="170dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="19dp"
            android:layout_marginTop="92dp"
            android:background="@drawable/mycard_p10_v1"
            android:clickable="true"
            android:onClick="p10_v1" />

        <ImageView
            android:id="@+id/iv_mycard_p10_v2"
            android:layout_width="258dp"
            android:layout_height="90dp"
            android:layout_alignLeft="@+id/iv_mycard_p10_v1"
            android:layout_below="@+id/iv_mycard_p10_v1"
            android:background="@drawable/mycard_p10_v2"
            android:clickable="true"
            android:onClick="p10_v1" />

        <TextView
            android:id="@+id/tv_p10_t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/iv_mycard_p10_v2"
            android:layout_marginBottom="30dp"
            android:clickable="true"
            android:onClick="p10_t1"
            android:text="可编辑"
            android:textColor="#ff8d09"
            android:textSize="25sp" />
    </RelativeLayout>
</LinearLayout>