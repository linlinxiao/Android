<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_right_row9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="CMF营销主管编辑完成-9"
            android:textColor="#000000" />

        <CheckBox android:textSize="12sp"
            android:id="@+id/cb_mycard_left_row9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:text="没有该项荣誉"
            android:textColor="#000000" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_mycard_row9"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:background="@drawable/mycard_back" >

        <ImageView
            android:id="@+id/iv_mycard_p9_v1"
            android:layout_width="150dp"
            android:layout_height="170dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="100dp"
            android:background="@drawable/mycard_p3_v1"
            android:clickable="true"
            android:onClick="p9_v1" />

        <TextView
            android:id="@+id/tv_p9_t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/iv_mycard_p9_v1"
            android:layout_marginRight="22dp"
            android:layout_marginTop="83dp"
            android:clickable="true"
            android:onClick="p9_t1"
            android:text="可编辑"
            android:textColor="#ff8d09"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/tv_p9_t2"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/tv_p9_t1"
            android:layout_marginBottom="21dp"
            android:clickable="true"
            android:gravity="right"
            android:onClick="p9_t2"
            android:text="可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑可编辑"
            android:textColor="#ffffff"
            android:textSize="14sp" />
    </RelativeLayout>
</LinearLayout>